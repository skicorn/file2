<link href="~/Content/Css/Login-Register.css" rel="stylesheet" />
<div class="register-page">
	<table>
		<!--USERNAME-->
		<tr>
			<td>@Html.TextBoxFor(s => s.userName, new { @class = "name", placeholder = "Username" })</td>
		</tr>
		<!--PASS-->
		<tr>
			<td>@Html.TextBoxFor(s => s.userPass, new { @class = "name", placeholder = "Password" })</td>
		</tr>
		<!--Repass-->
		<tr>
			<td>@Html.TextBoxFor(s => s.userRepass, new { @class = "name", placeholder = "Repassword" })</td>
		</tr>
		<tr>
			<td>
				<input type="submit" class="register-regispage" value="REGISTER" />
			</td>
		</tr>
	</table>
